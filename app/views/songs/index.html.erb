<h1>Songs:</h1>
<table>

  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>genre</th>
      <th>artist</th>
      <% @songs.each do |song| %>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><%=song.title%></td>
      <td><%=song.release_year%></td>
      <td><%=song.genre%></td>
      <td><%=song.artist_name%></td>
      <td>
        <%= link_to "View", song %>
        <%= link_to "Edit", edit_song_path(song) %>
        <%= link_to "Delete", song, method: :delete, data: {confirm: 'For Real?'} %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
